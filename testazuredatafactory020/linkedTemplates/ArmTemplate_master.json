{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testazuredatafactory020"
		},
		"LS_BlobConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobConnection'"
		},
		"LS_DatalakeGen2Connection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DatalakeGen2Connection'"
		},
		"LS_SQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLServer'",
			"defaultValue": "Integrated Security=True;Data Source=testsqlservervm;Initial Catalog=testdb"
		},
		"LS_SQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQLServer'"
		},
		"LS_BLOB_Connection_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "BlobStorageconnectionString"
		},
		"LS_DatalakeGen2Connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testdatalakege2020.dfs.core.windows.net/"
		},
		"LS_Datalake_Connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testdatalakege2020.dfs.core.windows.net/"
		},
		"LS_HTTPConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_KeyvaultLinkedService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://testkeyvaultdemo021.vault.azure.net/"
		},
		"LS_SQL Connection_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SQLConnectionString"
		},
		"LS_SQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Suresh"
		},
		"EVENT_TRIGGER_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0b09d9a2-10b0-4bdb-966c-fa3ebfa3ba0a/resourceGroups/TestRG002/providers/Microsoft.Storage/storageAccounts/testblobstorage020"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testazuredatafactory020_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BlobConnection_connectionString": {
						"value": "[parameters('LS_BlobConnection_connectionString')]"
					},
					"LS_DatalakeGen2Connection_accountKey": {
						"value": "[parameters('LS_DatalakeGen2Connection_accountKey')]"
					},
					"LS_SQLServer_connectionString": {
						"value": "[parameters('LS_SQLServer_connectionString')]"
					},
					"LS_SQLServer_password": {
						"value": "[parameters('LS_SQLServer_password')]"
					},
					"LS_DatalakeGen2Connection_properties_typeProperties_url": {
						"value": "[parameters('LS_DatalakeGen2Connection_properties_typeProperties_url')]"
					},
					"LS_HTTPConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTPConnection_properties_typeProperties_url')]"
					},
					"LS_KeyvaultLinkedService_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyvaultLinkedService_properties_typeProperties_baseUrl')]"
					},
					"LS_BLOB_Connection_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_BLOB_Connection_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_Datalake_Connection_properties_typeProperties_url": {
						"value": "[parameters('LS_Datalake_Connection_properties_typeProperties_url')]"
					},
					"LS_SQL Connection_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_SQL Connection_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_SQLServer_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "testazuredatafactory020_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"EVENT_TRIGGER_properties_typeProperties_scope": {
						"value": "[parameters('EVENT_TRIGGER_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testazuredatafactory020_ArmTemplate_0')]"
			]
		},
		{
			"name": "testazuredatafactory020_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testazuredatafactory020_ArmTemplate_1')]"
			]
		}
	]
}